{
  "workbench.fontAliasing": "antialiased",
  "workbench.preferredDarkColorTheme": "Aura Dark (Soft Text)",
  "workbench.editor.decorations.colors": true,
  "workbench.view.alwaysShowHeaderActions": false,
  "workbench.editor.closeOnFileDelete": true,
  "editor.formatOnSave": true,
  "diffEditor.codeLens": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.defaultColorDecorators": true,
  "editor.fontFamily": "Geist Mono",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.experimental.tsserver.web.typeAcquisition.enabled": true,
  "typescript.disableAutomaticTypeAcquisition": false,
  "js/ts.implicitProjectConfig.target": "ESNext",
  "typescript.tsserver.enableTracing": true,
  "workbench.editor.navigationScope": "editorGroup",
  "typescript.tsdk": "node_modules/typescript/lib"
}
